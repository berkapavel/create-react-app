{
  "name": "react-todo",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-plugin-relay": "^10.1.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.10.18",
    "prettier": "^2.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "tsc",
    "start": "concurrently \"tsc -w\" \"nodemon dist/js/app.js\""
  },
  "eslintConfig": {
    "rules": {},
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {}
      }
    ]
  },
  "devDependencies": {
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/mongoose": "^5.10.1",
    "@types/node": "^12.19.7",
    "@types/react": "^16.14.2",
    "@types/react-dom": "^16.9.10",
    "@types/react-router-dom": "^5.1.6",
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.6"
  }
}
